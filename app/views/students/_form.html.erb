<div class="form-div">
    <%= form_for(student) do |f| %>
    <div>
        <%= f.label 'Name' %>
        <%= f.text_field :name %>
    </div><br>

    <div>
         <%= f.label 'Category' %>
         <%= f.text_field :category %>
    </div>
    <br>

    <div>
        <%= f.label 'Level' %>
        <%= f.text_field :level %>
    </div><br>


        <%= f.submit %>
    <br><br>

    <% end %>
</div>